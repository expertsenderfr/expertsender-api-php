{
  "name": "expertsenderfr/expertsender-api-php",
  "description": "A PHP wrapper for the ExpertSender API",
  "type": "library",
  "authors": [
    {
      "name": "Isaac Rozas",
      "email": "contact@expertsender.fr"
    }
  ],
  "autoload": {
    "psr-4": {
      "ExpertSenderFr\\ExpertSenderApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ExpertSenderFr\\ExpertSenderApi\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^5.6|^7.0",
    "symfony/options-resolver": "~2.8 | ^3.4",
    "symfony/serializer": "~2.8 | ^3.4",
    "symfony/property-access": "~2.8 | ^3.4",
    "symfony/dom-crawler": "~2.8 | ^3.4",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "roave/security-advisories": "dev-master"
  }
}
